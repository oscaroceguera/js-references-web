ngTag,\n    data: dataTag,\n    error: errorTag,\n  } = useQuery<ITags>(GET_TAGS, {\n    pollInterval: 200,\n  });\n  const [state, dispatch] = React.useReducer(reducer, INITIAL_STATE);\n\n  console.log({ state });\n\n  const goBack = (event: MouseEvent<HTMLButtonElement>) => {\n    event.preventDefault();\n    history.push('/');\n  };\n\n  const handleField = useCallback<CallbackType>(\n    (event: FormEvent<HTMLInputElement | HTMLTextAreaElement>) => {\n      event.preventDefault();\n\n      const payload = {\n        field: event.currentTarget.id,\n        value: event.currentTarget.value,\n      };\n\n      dispatch(setFields(payload));\n    },\n    [],\n  );\n\n  const onChangeSelect = useCallback<CallbackType>((e: any) => {\n    const payload = {\n      field: e.name,\n      value: e.value,\n    };\n\n    dispatch(setFields(payload));\n  }, []);\n\n  const onChangeTags = useCallback<CallbackType>((e: any) => {\n    const payload = {\n      field: 'tags',\n      value: e ? e.map((item: IOption) => item.value) : [],\n    };\n\n    dispatch(setFields(payload));\n  }, []);\n\n  const handleSubmit = () => {\n    console.log('handleSubmit');\n  };\n\n  const isVisible = useMemo<boolean>(() => {\n    const { title, tags, category, content } = state;\n    const isTagsEmpty = typeof tags !== 'undefined' && tags.length > 0;\n    return !!title && !!category && !!content && isTagsEmpty;\n  }, [state]);\n\n  const isLoading = useMemo<boolean>(() => {\n    return loadingCategory || loadingTag;\n  }, [loadingTag, loadingCategory]);\n\n  const isError = useMemo<unknown>(() => {\n    return errorCategory || errorTag;\n  }, [errorCategory, errorTag]);\n\n  return (\n    <Container onSubmit={handleSubmit}>\n      <GoBack onClick={goBack}>Back</GoBack>\n      {isLoading && !isError && <Loader secondary />}\n      {isError && !isLoading && <ErrorMsg />}\n      {!isLoading && !isError && (\n        <>\n          <Title>Editor</Title>\n          <EditorContainer>\n            <Field\n              id=\"title\"\n              label=\"Title\"\n              value={state.title}\n              required\n              onChange={handleField}\n            />\n            <SelectCatalog\n              data={dataCategory ? dataCategory.categories : undefined}\n              field=\"category\"\n              required={!state.category}\n              onChange={onChangeSelect}\n            />\n            <br />\n            <SelectCatalog\n              data={dataTag ? dataTag.tags : undefined}\n              field=\"tags\"\n              isMulti\n              required={state.tags?.length === 0}\n              onChange={onChangeTags}\n            />\n            <br />\n            <Field\n              id=\"content\"\n              label=\"Content\"\n              kind=\"textarea\"\n              placeholder=\"MD content...\"\n              value={state.content}\n              required\n              onChange={handleField}\n            />\n            <br />\n            <BtnSave\n              type=\"submit\"\n              value=\"Save\"\n              disabled={!isVisible}\n              isVisible={isVisible}\n            />\n          </EditorContainer>\n        </>\n      )}\n    </Container>\n  );\n};\n\nexport default Editor;\n","/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/pages/Home/styles.ts",[],[],"/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/components/Header/styles.tsx",[],"/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/components/Loader/styles.tsx",[],"/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/config/apolloClient.ts",[],"/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/globalCSS.ts",[],"/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/components/Posts/index.tsx",[],"/Users/oscaroceguerabibriesca/Documents/Repos/projects/js-references-web/src/components/Pos